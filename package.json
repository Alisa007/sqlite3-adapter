{
  "name": "waterline-sqlite3",
  "description": "Waterline Adapter for SQLite3",
  "version": "1.0.1",
  "author": "Travis Webb <tjwebb@balderdash.io>",
  "url": "http://github.com/waterlinejs/sqlite3-adapter",
  "keywords": [
    "sqlite",
    "sqlite3",
    "node orm",
    "orm",
    "waterline",
    "waterlinejs",
    "driver",
    "sails",
    "sailsjs",
    "es6"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/waterlinejs/sqlite3-adapter.git"
  },
  "dependencies": {
    "knex": "^0.9",
    "lodash": "^3.10.0",
    "moment": "^2.10.6",
    "sqlite3": "^3.1.3",
    "waterline-cursor": "0.0.6",
    "waterline-errors": "^0.10.1",
    "waterline-sequel": "^0.5.0"
  },
  "devDependencies": {
    "babel": "^5.6.23",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.2.0",
    "mocha": "latest",
    "waterline-adapter-tests": "waterlinejs/waterline-adapter-tests"
  },
  "bundledDependencies": [
    "knex",
    "lodash",
    "moment",
    "sqlite3",
    "waterline-cursor",
    "waterline-errors",
    "waterline-sequel"
  ],
  "scripts": {
    "test": "gulp && rm -rf .tmp/ ; NODE_ENV=test node test/integration/runner.js",
    "prepublish": "gulp"
  },
  "main": "dist/adapter",
  "license": "MIT",
  "bugs": "https://github.com/waterlinejs/sqlite3-adapter/issues",
  "waterlineAdapter": {
    "waterlineVersion": ">0.10.0",
    "interfaces": [
      "associations",
      "migratable",
      "queryable",
      "semantic",
      "sql"
    ],
    "features": [
      "autoIncrement",
      "crossAdapter",
      "unique"
    ]
  },
  "engines": {
    "node": ">= 0.12.0"
  }
}
